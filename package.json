  "name": "dependencies-cli",
  "version": "2.0.0-beta",
  "author": "Andrew Fawcett, Justin Kalloor, Holger Kache",
  "bugs": "https://github.com/afawcett/dependencies-cli/issues",
  "dependencies": {
    "@oclif/config": "^1.6.27",
    "@oclif/errors": "^1.0.9",
    "@salesforce/command": "^1.2.0",
    "@salesforce/core": "^1.2.0",
